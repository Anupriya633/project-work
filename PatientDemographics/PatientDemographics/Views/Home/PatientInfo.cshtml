        $.getJSON(uri)
            .done(function (data) {
                //  $("$#patient").show();
                $.each(data, function (key, item) {
                    $('#patient').append("<tr><td>" + item.ForeName + "</td>" + "<td>" + item.Surname + "</td>" + "<td>" + item.DateOfBirth + "</td>" + "<td>" + item.Gender + "</td></tr>");
                });
            });
    });
